{
  "name": "Trybe Clock - test",
  "description": "gets agenda from slack",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./src/index.html"
  },
  "background": {
    "service_worker": "./src/background.js",
    "type": "module"
  },
  "permissions": ["alarms", "activeTab", "scripting", "storage", "notifications", "system.display"],
  "content_scripts": [
    {
      "matches": ["https://app.slack.com/*"],
      "js": ["./src/content.js"]
    }
  ]
}